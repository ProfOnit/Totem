e\");\r\n\r\n    }\r\n\r\n    tick() {\r\n        this.secs--;\r\n        if (this.secs == 0) {\r\n           this.movementManager();\r\n           this.secs = 60;\r\n        }\r\n    }\r\n\r\n    update() {\r\n        var timer = this.time.addEvent({\r\n            delay: 1000,\r\n            callback: this.tick,\r\n            callbackScope: this,\r\n            loop: true\r\n        });\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"771a6366fc99bdbfe2e51b8b7acd7788","cacheData":{"env":{}}}